   "source": [
    "count = []\n",
    "for i,num in enumerate(nums):\n",
    "    if num == 0:\n",
    "        count.append(i)\n",
    "        \n",
    "temp = 0\n",
    "for i in count:\n",
    "    index = i-temp\n",
    "    del nums[index]\n",
    "    temp+=1\n",
    "    \n",
    "nums.extend([0]*len(count))"
   ]
